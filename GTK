import gi
gi.require_version('Gtk', '3.0')
gi.require_version('GtkSource', '3.0')
from gi.repository import Gtk, GtkSource

builder = Gtk.Builder()
builder.add_from_file("test_ui.glade")

window = builder.get_object("window1")
notebook = builder.get_object("notebook1")
sourceview = builder.get_object("gtksourceview1")

if not sourceview:
    print("Error: GtkSourceView widget not found.")
else:
    print("GtkSourceView widget successfully loaded.")

window.show_all()
Gtk.main()
